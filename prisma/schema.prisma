generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model make {
  id     Int      @id(map: "Make_pkey") @default(autoincrement())
  make   String   @unique(map: "Make_make_key")
  logo   String
  models models[]
}

model models {
  id           Int       @id(map: "Models_pkey") @default(autoincrement())
  makeId       Int
  model        String
  regNo        String    @unique(map: "Models_regNo_key") @db.VarChar(7)
  mileage      Int
  bodyType     String
  seatCapacity Int
  rate         Float
  year         Int
  img          String
  booked_from  DateTime? @db.Timestamp(6)
  booked_to    DateTime? @db.Timestamp(6)
  make         make      @relation(fields: [makeId], references: [id], onDelete: Cascade, map: "Models_makeId_fkey")

  @@index([makeId], map: "Models_makeId_idx")
}
